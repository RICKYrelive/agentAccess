# FastGPT 部署配置文件
# 复制此文件并修改为你自己的配置

# PostgreSQL 配置
POSTGRES_USER=myuser
POSTGRES_PASSWORD=mypassword
POSTGRES_DB=fastgpt

# FastGPT 配置
FASTGPT_PORT=3000

# OpenAI API 配置（请替换为你自己的 API Key）
# 如果使用国内 API，可以修改 BASE_URL
OPENAI_API_BASE_URL=https://api.openai.com/v1
OPENAI_API_KEY=sk-your-api-key-here

# Token 密钥（请修改为随机字符串）
TOKEN_KEY=your-secret-key-change-this-to-random-string

# 全局限制
GLOBAL_API_RATE_LIMIT=100
GLOBAL_MAX_QUOTA=1000000
